{% extends 'template.html.twig' %}


{% block body %}


{% block PageTitle %}
    Liste des todos
{% endblock %}
<ol class="list-group">
    {% for name,content in app.session.get('todo') %}
        <li class="list-group-item">
            {{ name | capitalize }} : {{ content | capitalize }}
        </li>
    {% endfor %}
</ol>
{% endblock %}

{% block title %}
    Todo
{% endblock %}